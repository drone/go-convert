version: 1

stages:
- desc: converted from travis.yml
  name: pipeline
  type: ci
  spec:
    steps:
    - name: homebrew
      type: script
      spec:
        run: brew bundle --file=Brewfile.travis
    - name: install
      type: script
      spec:
        run: bundle install --jobs=3 --retry=3
    - name: script
      type: script
      spec:
        run: rake
