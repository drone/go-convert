version: 1

stages:
- desc: converted from travis.yml
  name: pipeline
  type: ci
  spec:
    envs:
      TRAVIS_GO_VERSION: <+matrix.go>
    steps:
    - name: install
      type: script
      spec:
        run: go get ./...
    - name: install1
      type: script
      spec:
        run: go install .
    - name: script
      type: script
      spec:
        run: goveralls
  strategy:
    spec:
      axis:
        go:
        - "1.16"
        - "1.15"
    type: matrix

